   "source": [
    "from math import comb\n",
    "import random as rd\n",
    "\n",
    "def prob(n, r):\n",
    "    val = pow((1 - 1/n), n-r)\n",
    "    val2 = pow((1/n), r)\n",
    "    return comb(n, r) * val2 * val\n",
    "print(prob(2,3))"
   ]
